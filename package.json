{
  "name": "watch-discord-dev-changes",
  "version": "2.0.0",
  "description": "When an update is made to Discord's Developer Change Log, the specific channel on Discord will be notified.",
  "homepage": "https://github.com/tomacheese/watch-discord-dev-changes",
  "bugs": {
    "url": "https://github.com/tomacheese/watch-discord-dev-changes/issues"
  },
  "license": "MIT",
  "author": "Tomachi <tomachi@tomacheese.com>",
  "private": true,
  "main": "dist/main.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:tomacheese/watch-discord-dev-changes.git"
  },
  "scripts": {
    "fix:prettier": "prettier --write src",
    "fix:eslint": "eslint . -c eslint.config.mjs --fix",
    "lint:prettier": "prettier --check src",
    "fix": "run-z fix:prettier fix:eslint",
    "lint:tsc": "tsc",
    "test": "jest --runInBand --passWithNoTests --detectOpenHandles --forceExit",
    "lint:eslint": "eslint . -c eslint.config.mjs",
    "dev": "tsx watch ./src/main.ts",
    "start": "tsx ./src/main.ts",
    "lint": "run-z lint:prettier,lint:eslint,lint:tsc"
  },
  "devDependencies": {
    "@book000/eslint-config": "1.11.2",
    "@book000/node-utils": "1.18.125",
    "@types/jest": "30.0.0",
    "@types/node": "24.7.0",
    "axios": "1.12.2",
    "date-fns": "4.1.0",
    "eslint": "9.37.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-n": "17.23.1",
    "eslint-plugin-promise": "7.2.1",
    "jest": "30.2.0",
    "jest-expect-message": "1.1.3",
    "prettier": "3.6.2",
    "run-z": "2.1.0",
    "ts-jest": "29.4.4",
    "tsx": "4.20.6",
    "typescript": "5.9.3"
  },
  "jest": {
    "testMatch": [
      "**/*.test.ts"
    ],
    "setupFilesAfterEnv": [
      "jest-expect-message"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json"
        }
      ]
    }
  },
  "packageManager": "pnpm@10.18.1+sha512.77a884a165cbba2d8d1c19e3b4880eee6d2fcabd0d879121e282196b80042351d5eb3ca0935fa599da1dc51265cc68816ad2bddd2a2de5ea9fdf92adbec7cd34"
}
